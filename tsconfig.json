{
    "compilerOptions": {
        "experimentalDecorators": true,
        "target": "ES2018",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "lib": [
        "ESNext",
        "ESNext.AsyncIterable",
        "DOM"
        ],
        "esModuleInterop": true,
        "allowJs": true,
        "sourceMap": true,
        "strict": true,
        "noEmit": true,
        "baseUrl": ".",
        "noImplicitAny": false,
        "paths": {
        "~/*": [
            "./*"
        ],
        "@/*": [
            "./*"
        ]
        },
        "types": [
        "@nuxt/types"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}